---
layout: null
---
<?xml version="1.0" encoding="UTF-8"?>
<urlset
    xmlns="http://www.sitemaps.org/schemas/sitemap/0.9"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.sitemaps.org/schemas/sitemap/0.9
       http://www.sitemaps.org/schemas/sitemap/0.9/sitemap.xsd">	
{% for post in site.posts  %}
{% if forloop.first %}
<url>
<loc>{{ site.domainurl }}</loc>
<priority>1.00</priority>
<lastmod>{{ post.date | date: "%Y-%m-%d" }}</lastmod>
<changefreq>daily</changefreq>
</url>

<url>
<loc>{{ site.domainurl }}/pages/categories</loc>
<priority>1.00</priority>
<lastmod>{{ post.date | date: "%Y-%m-%d" }}</lastmod>
<changefreq>daily</changefreq>
</url>

<url>
<loc>{{ site.domainurl }}/pages/search</loc>
<priority>1.00</priority>
<lastmod>{{ post.date | date: "%Y-%m-%d" }}</lastmod>
<changefreq>daily</changefreq>
</url>

<url>
<loc>{{ site.domainurl }}/pages/links</loc>
<priority>1.00</priority>
<lastmod>{{ post.date | date: "%Y-%m-%d" }}</lastmod>
<changefreq>daily</changefreq>
</url>

<url>
<loc>{{ site.domainurl }}/pages/chat</loc>
<priority>1.00</priority>
<lastmod>{{ post.date | date: "%Y-%m-%d" }}</lastmod>
<changefreq>daily</changefreq>
</url>

<url>
<loc>{{ site.domainurl }}/pages/about</loc>
<priority>1.00</priority>
<lastmod>{{ post.date | date: "%Y-%m-%d" }}</lastmod>
<changefreq>daily</changefreq>
</url>

{% endif %}

<url>
<loc>{{ site.domainurl }}{{ site.baseurl }}{{ post.url }}</loc>
<priority>0.5</priority>
<lastmod>{{ post.date | date: "%Y-%m-%d" }}</lastmod>
<changefreq>monthly</changefreq>
</url>

{% endfor %}
</urlset>
